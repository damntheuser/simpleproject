	<script language="javascript" type="text/javascript">
		var target = new Date('04/25/2012'),
    today = new Date(),
    daysToGo = Math.ceil((target.getTime() - today.getTime() ) / (1000*60*60*24)),
    // probably not the best, but it should work
    percent = 100 - parseInt( ( daysToGo / 80) * 100, 10);

$("#progressbar").progressbar({
    value: percent,
    create: function(event, ui) {
        $('#progressbar').after(daysToGo + ' days left!');
    }
});
		</script>
